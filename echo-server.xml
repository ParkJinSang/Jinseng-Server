<?xml version="1.0" encoding="UTF-8"?>
<config>
	<server id="echo">
		<name>EchoServer</name>
		<type>HTTP</type>
		<host>127.0.0.1</host>
		<port>8080</port>
		<timeout>60000</timeout>
		<useSSL>true</useSSL>
		<services>
			<service tag="1">
				<path>src/sample/http/EchoWebService.java</path>
				<class>sample.http.EchoWebService</class>
			</service>
		</services>
		<proxy>
			<host></host>
			<port></port>
		</proxy>
	</server>
</config>